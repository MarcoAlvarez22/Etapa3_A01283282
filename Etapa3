        "def leer_archivo(nombre): #Fuente: el prof   agregue la variable preguntas como un lista en blanco par que la lista haga reset y poder usar la repeticon de la funcion actualizar pregunta\n",
        "  \n",
        "  global preguntas\n",
        "  preguntas = []\n",
        "  with open(nombre, \"r\") as archivo:  # with open cierra automáticamente el archivo, pero para poder leerlo, primero debemos crear o subir el archivo a colab\n",
        "    \n",
        "    contenido = archivo.readlines() # lee todas las líneas del archivo de texto\n",
        "\n",
        "    for elemento in contenido: # para recorrer cada línea del contenido\n",
        "      linea = elemento.split(\",\") # crear una lista con todos los elementos de la línea, cada coma es un elemento\n",
        "      linea[7] = linea[7].replace(\"\\\\n\", \"\") # borrar la \\n del último elemento\n",
        "      preguntas.append(linea)# agregar la línea (lista) a la lista de preguntas\n",
        "\n",
        "def guardar_archivo(nombre): #la funcion guardar archivo toma la matriz preguntas y cada lista la convierte en un string y la escribe una por linea en el archivo de texto preguntas.txt, a su vez elimina todos los simbolos y espacios que pueden afectar el texto entre \n",
        "#los cambios de formato\n",
        "\n",
        "  global preguntas\n",
        "\n",
        "  with open(nombre, \"w\") as archivo:\n",
        "    for num in range(len(preguntas)):\n",
        "      lista_n = preguntas[num]\n",
        "      if lista_n == preguntas[-1]:\n",
        "        lista_n = str(lista_n).strip().replace(\"\\\\n\", \"\").replace(\"'\", \"\").replace(\"[\", \"\").replace(\"]\", \"\").replace(\"  \", \" \").replace(\", \", \",\")\n",
        "        archivo.write(f\"{lista_n}\")\n",
        "      else:\n",
        "        lista_n = str(lista_n).replace(\"\\\\n\", \"\").replace(\"'\", \"\").replace(\"[\", \"\").replace(\"]\", \"\").replace(\"  \", \" \").replace(\", \", \",\")\n",
        "        archivo.write(f\"{lista_n}\\n\")\n",
        "\n",
        "def menu(): #La funcion menu se encarga de desplegar la lista menu de tal forma que cada numero del 1 al 6 le corresponde a cada opcion, despues regresa el input del usuario segun la opcion que eliga a la funcion main\n",
        "\n",
        "  for espacio in range(1):\n",
        "    print(\" \") \n",
        "\n",
        "  for num in range(2, len(lista_menu)):\n",
        "    print(\"Opcion[\" + str(num - 1) + \"] - \" + str(lista_menu[num]))\n",
        "\n",
        "  for espacio in range(1):\n",
        "    print(\" \")\n",
        "\n",
        "  return input(\"Selecciona el número de opcion: \")\n",
        "\n",
        "  for espacio in range(1):\n",
        "    print(\" \")\n",
        "\n",
        "def main(): #La funcion main toma la respuesta devuelta por la función menu y la evalua para saber que opcion del 1 al 6 elegíste, si se elige una opcion que no está dentro de ese rango te lo hace saber y se repite el proceso\n",
        "\n",
        "  while True:\n",
        "\n",
        "    opcion = menu()\n",
        "\n",
        "    if opcion == \"1\":\n",
        "      registrar_pregunta()\n",
        "\n",
        "    elif opcion == \"2\":\n",
        "      actualizar_pregunta()\n",
        "\n",
        "    elif opcion == \"3\":\n",
        "      estudiar_area()\n",
        "\n",
        "    elif opcion == \"4\":\n",
        "      presentar_quiz()\n",
        "\n",
        "    elif opcion == \"5\":\n",
        "      reportar_calificaciones()\n",
        "\n",
        "    elif opcion == \"6\":\n",
        "      salir()\n",
        "      break\n",
        "    else:\n",
        "      print(\"La opcion seleccionada no existe, por favor ingrese una opcion valida\")\n",
        "      continue\n",
        "  \n",
        "def registrar_pregunta(): # la funcion registrar pregunta funciona con un ciclo ehile, por medio del cual mientras el valor de nueva_pregunta sea si, te edira datos para registrar una nueva pregunta, primero el id, despues desplegara los enfoques en un \n",
        "# menu de tal forma que sea más facil escoger el enfoque, despues te pide el texto de la pregunta, las 4 posibles respuestas y el inciso de la respuesta correcta, al terminar esto crea un texto con todos estos elementos separados por comas\n",
        "# este texto lo convierte en una lista, lo anexa a la lista de preguntas y se graba un archivo con las preguntas en la lista.\n",
        "  nueva_pregunta = \"SI\"\n",
        "  global lista_letras\n",
        "  global enfoques\n",
        "  leer_archivo(\"preguntas.txt\")\n",
        "  while nueva_pregunta == \"SI\": \n",
        "\n",
        "    print(f'''\\nHas selecicionado la opción de registrar pregunta, \n",
        "    por favor sigue las instrucciones para poder completar el registro\\n''') \n",
        "    \n",
        "    id = int(input(\"Ingrese el ID de la pregunta que desea registrar: \"))\n",
        "\n",
        "    for espacio in range(1):\n",
        "      print(\" \")\n",
        "\n",
        "    for materia in range(len(enfoques)):\n",
        "        print(str(enfoques[materia]) + \" [\" + str(materia + 1) + f\"]\")\n",
        "\n",
        "    \n",
        "    enf_pregunta = input(f\"\\nPor favor, Ingrese el numero que corresponda al enfoque de su pregunta: \")\n",
        "\n",
        "    while enf_pregunta != \"1\" and enf_pregunta != \"2\" and enf_pregunta != \"3\":\n",
        "      enf_pregunta = input(f\"\\nPor favor, Ingrese el numero que corresponda al enfoque de su pregunta: \")\n",
        "\n",
        "    if enf_pregunta == \"1\":\n",
        "      enfoque = \"Lectura\"\n",
        "\n",
        "    elif enf_pregunta == \"2\":\n",
        "      enfoque = \"Matemáticas\"\n",
        "\n",
        "    elif enf_pregunta == \"3\":\n",
        "        enfoque = \"Ciencias\"\n",
        "\n",
        "    t_pregunta = input(f\"\\nIngrese el texto de la pregunta a registrar: \")\n",
        "\n",
        "    respuestas = \"\"\n",
        "\n",
        "    for num in lista_letras:\n",
        "      respuesta = input(\"Ingresa la respuesta \" + str(num) + \": \")\n",
        "\n",
        "      if num == 3:\n",
        "        respuestas = respuestas + respuesta \n",
        "      else:\n",
        "        respuestas = respuestas + respuesta + \",\"\n",
        "\n",
        "    respuestas = respuestas.split(\",\")\n",
        "\n",
        "    r_correcta = input(\"Por favor, Ingrese el inciso de la respuesta correcta: \")\n",
        "\n",
        "    if id == 1:\n",
        "        texto = f\"{str(id)},{str(enfoque)},{str(t_pregunta)},{str(respuestas[0])},{str(respuestas[1])},{str(respuestas[2])},{str(respuestas[3])},{str(r_correcta)}\"\n",
        "        \n",
        "    else:\n",
        "      texto = f\"\\n{str(id)},{str(enfoque)},{str(t_pregunta)},{str(respuestas[0])},{str(respuestas[1])},{str(respuestas[2])},{str(respuestas[3])},{str(r_correcta)}\"\n",
        "      \n",
        "    preguntas.append([texto])\n",
        "\n",
        "    guardar_archivo(\"preguntas.txt\")\n",
        "\n",
        "    nueva_pregunta = input(f\"\\n¿Desea registrar otra pregunta?\").upper()\n",
        "\n",
        "def actualizar_pregunta(): #la funcion actualizar pregunta se encarga de asignar un nombre a cada elemento de las lista de la matriz preguntas, despues las imprime en un formato bonita para que el usuario las pueda ver, ahí le pide al usuario que ingrese el id \n",
        "#de la pregunta que desea cambiar, le pide los datos cambiable, todos menos el id, que serian el enfoque, el texto, las respuestas y la respuestas correctas, despues vuelve a realizar la asignación y despliega la pregunta nueva, pregunta al usuario si desea realizar\n",
        "#el cambio y si la respuesta es si, se guarda la nueva pregunta en el archivo de texto, despues pregunta si deseas actualizar alguna otra pregunta, si la respuesta es si se repite, si es no u otra te devuelve al menu\n",
        "  \n",
        "  leer_archivo(\"preguntas.txt\")\n",
        "\n",
        "  print(\"Bienvenido a la opción para actualizar preguntas, por favor sigue las instrucciones \\nA continuación se despliega la lista de preguntas guardadas:\\n\")\n",
        "\n",
        "  for pregunta in preguntas:\n",
        "\n",
        "    id, enfoque, pregunta, respuesta1, respuesta2, respuesta3, respuesta4, correcta = pregunta\n",
        "\n",
        "    print(f\"ID de la pregunta: {id} \\nEnfoque de la pregunta: {enfoque} \\nPregunta: {pregunta}\")\n",
        "    print(f\"a){respuesta1:<25}b){respuesta2:<25}c){respuesta3:<25}d){respuesta4:<25} \\n\")\n",
        "\n",
        "  id_cambio = input(\"Si no desea cambiar ninguna pregunta presion enter, de no ser así ingrese el ID de la pregunta que desea cambiar: \")\n",
        "\n",
        "  if id_cambio == \"\":\n",
        "    pass\n",
        "\n",
        "  else:\n",
        "    for materia in range(len(enfoques)):\n",
        "      print(str(enfoques[materia]) + \" [\" + str(materia + 1) + f\"]\")\n",
        "\n",
        "  \n",
        "    enf_pregunta = input(f\"\\nPor favor, Ingrese el numero que corresponda al enfoque de su pregunta: \")\n",
        "\n",
        "    while enf_pregunta != \"1\" and enf_pregunta != \"2\" and enf_pregunta != \"3\":\n",
        "\n",
        "      enf_pregunta = input(f\"\\nPor favor, Ingrese el numero que corresponda al enfoque de su pregunta: \")\n",
        "\n",
        "    if enf_pregunta == \"1\":\n",
        "      enfoque = \"Lectura\"\n",
        "\n",
        "    elif enf_pregunta == \"2\":\n",
        "      enfoque = \"Matemáticas\"\n",
        "\n",
        "    elif enf_pregunta == \"3\":\n",
        "      enfoque = \"Ciencias\"\n",
        "\n",
        "    t_pregunta = input(f\"\\nIngrese el texto de la pregunta a registrar: \")\n",
        "\n",
        "    respuestas = \"\"\n",
        "\n",
        "    for num in lista_letras:\n",
        "      respuesta = input(\"Ingresa la respuesta \" + str(num) + \": \")\n",
        "\n",
        "      if num == 3:\n",
        "        respuestas = respuestas + respuesta \n",
        "      else:\n",
        "        respuestas = respuestas + respuesta + \",\"\n",
        "\n",
        "    respuestas = respuestas.split(\",\")\n",
        "\n",
        "    r_correcta = input(\"Por favor, Ingrese la respuesta correcta: \")\n",
        "    \n",
        "    texto = f\"{str(id_cambio)}, {str(enfoque)}, {str(t_pregunta)}, {str(respuestas[0])}, {str(respuestas[1])}, {str(respuestas[2])}, {str(respuestas[3])}, {str(r_correcta)}\\n\"\n",
        "\n",
        "    pregunta_actualizada = [texto.split(\",\")]\n",
        "\n",
        "    print(f\"\\nPregunta actualizada: \")\n",
        "    print(f\"ID de la pregunta: {id_cambio} \\nEnfoque de la pregunta: {enfoque} \\nPregunta: {t_pregunta}\")\n",
        "    print(f\"a){respuestas[0]:<25}b){respuestas[1]:<25}c){respuestas[2]:<25}d){respuestas[3]:<25} \\n\")\n",
        "\n",
        "    guardar = input(\"¿Desea guardar el cambio? (Si/No)\").upper()\n",
        "\n",
        "    if guardar == \"SI\":\n",
        "      preguntas[int(id_cambio) - 1] = pregunta_actualizada\n",
        "\n",
        "    guardar_archivo(\"preguntas.txt\") \n",
        "      \n",
        "  cambio = input(f\"\\n¿Desea modificar algúna otra pregunta? (si/no) \").upper()\n",
        "  if cambio == \"SI\":\n",
        "    actualizar_pregunta()\n",
        "  else:\n",
        "    pass\n",
